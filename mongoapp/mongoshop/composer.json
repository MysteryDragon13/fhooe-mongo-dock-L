{
    "name": "fhooe/router-skeleton",
    "type": "project",
    "description": "An example application or skeleton for getting started with fhooe/router.",
    "keywords": [
        "routing",
        "example",
        "education"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Wolfgang Hochleitner",
            "email": "wolfgang.hochleitner@fh-hagenberg.at",
            "role": "Developer"
        },
        {
            "name": "Martin Harrer",
            "email": "martin.harrer@fh-hagenberg.at",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "fhooe/router": "^0.3",
        "monolog/monolog": "^2.3",
        "twig/twig": "^3.3",
        "twbs/bootstrap": "^5.1",
        "twbs/bootstrap-icons": "^1.7",
        "doctrine/orm": "^2.11.0",
        "doctrine/dbal": "^3.2",
        "doctrine/annotations": "1.13.2",
        "doctrine/mongodb-odm": "^2.4.2",
        "symfony/yaml": "^5.4",
        "symfony/cache": "^5.4",
        "mongodb/mongodb": "^1.0.0",
      "ext-mongodb": "*"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.2"
    },
    "minimum-stability": "stable",
    "autoload": {
            "psr-4": {
            "": ["", "src/", "tests/"]
        }
    },
    "scripts": {
        "start": [
            "echo 'Web server launched on http://localhost:8888'",
            "php -S localhost:8888 -t public"
        ],
        "test": [
            "@phpstan"
        ],
        "phpstan": "phpstan analyse public views --memory-limit=-1 --level 9"
    }
}